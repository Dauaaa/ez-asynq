{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "declaration": true,
    "outDir": "./dist",
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "paths": {
      "ez-asynq": [
        "./index.ts"
      ]
    }
  },
  "include": [
    "./internal/**/*.ts",
    "./index.ts"
  ]
}
